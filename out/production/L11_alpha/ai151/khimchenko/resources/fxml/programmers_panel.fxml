    <Button maxHeight="Infinity" maxWidth="Infinity" text="B" GridPane.columnIndex="2" GridPane.rowIndex="1"/>

    <Button maxHeight="Infinity" maxWidth="Infinity" text="RoL" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <Button maxHeight="Infinity" maxWidth="Infinity" text="RoR" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <Button maxHeight="Infinity" maxWidth="Infinity" text="C" GridPane.columnIndex="2" GridPane.rowIndex="2"/>

    <Button maxHeight="Infinity" maxWidth="Infinity" text="Or" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
