    <Button maxHeight="Infinity" maxWidth="Infinity" text="D" GridPane.columnIndex="2" GridPane.rowIndex="3"/>

    <Button maxHeight="Infinity" maxWidth="Infinity" text="Lsh" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
    <Button maxHeight="Infinity" maxWidth="Infinity" text="Rsh" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
    <Button maxHeight="Infinity" maxWidth="Infinity" text="E" GridPane.columnIndex="2" GridPane.rowIndex="4"/>

    <Button maxHeight="Infinity" maxWidth="Infinity" text="Not" GridPane.columnIndex="0" GridPane.rowIndex="5"/>
    <Button maxHeight="Infinity" maxWidth="Infinity" text="And" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
    <Button maxHeight="Infinity" maxWidth="Infinity" text="F" GridPane.columnIndex="2" GridPane.rowIndex="5"/>
